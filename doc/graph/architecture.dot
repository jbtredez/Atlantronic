	// liens
	strategy -> control
	strategy -> pince
	control -> adc
	control -> gpio
	control -> location
	control -> trajectory
	control -> usb
	trajectory -> detection
	location -> odometry
	location -> beacon
	pince -> ax12
	detection -> hokuyo
